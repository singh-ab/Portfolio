{
  "version": 2,
  "cleanUrls": true,
  "routes": [
    { "src": "/((?:assets|style|javascript)/.*)", "dest": "./$1" },
    { "src": "/[^.]+", "dest": "/index.html" }
  ],
  "headers": [
    {
      "source": "/(.*)\\.(css|js)",
      "headers": [{ "key": "Cache-Control", "value": "public, max-age=3600" }]
    }
  ]
}
